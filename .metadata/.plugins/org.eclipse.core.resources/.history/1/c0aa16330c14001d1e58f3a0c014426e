package lambdas;

public class Soma implements Calculo{
	public double a;
	public double b;
	
	
	@Override
	public double executar(double a, double b) {
		return this.a + this.b;
	}

	@Override
	public String toString() {
	
		return "A soma de " + this.a + " + " + this.b +  "Ã©: " + (this.a + this.b) ;
	}
	
	

}
