package lambdas;

public class Soma implements Calculo{
	public double a;
	public double b;
	
	Soma(double a, double b){
		this.a = a;
		this.b = b;
	}
	
	@Override
	public double executar(double a, double b) {
		return this.a + this.b;
	}

	@Override
	public String toString() {
	
		return "A soma de " + this.a + " + " + this.b +  "Ã©" ;
	}
	
	

}
